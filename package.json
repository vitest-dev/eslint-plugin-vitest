{
  "name": "@vitest/eslint-plugin",
  "version": "1.3.24",
  "license": "MIT",
  "description": "ESLint plugin for Vitest",
  "repository": "vitest-dev/eslint-plugin-vitest",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint plugin",
    "vitest",
    "vitest eslint plugin"
  ],
  "author": "Verite Mugabo <https://veritemugabo.com/>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --format esm --format cjs",
    "typecheck": "tsc --noEmit",
    "lint": "concurrently --prefixColors auto \"pnpm:lint:*\"",
    "lint:eslint-docs": "pnpm build && eslint-doc-generator --check",
    "lint:js": "eslint",
    "release": "bumpp && pnpm build && pnpm publish",
    "format": "prettier 'src/**/*.{ts,js}' --check",
    "test": "vitest",
    "update:chains": "node scripts/chain-permutations.ts",
    "update:eslint-docs": "pnpm build && eslint-doc-generator"
  },
  "dependencies": {
    "@typescript-eslint/scope-manager": "^8.46.1",
    "@typescript-eslint/utils": "^8.46.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.18.10",
    "@typescript-eslint/parser": "^8.46.1",
    "@typescript-eslint/rule-tester": "^8.46.1",
    "bumpp": "^10.3.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-doc-generator": "^2.3.0",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-remote-tester": "^4.0.3",
    "eslint-remote-tester-repositories": "^2.0.2",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0",
    "typescript": ">=5.0.0",
    "vitest": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "vitest": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "endOfLine": "auto"
  }
}
